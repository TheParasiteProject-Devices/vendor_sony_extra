<?xml version='1.0' encoding='utf-8' ?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:id="@android:id/background">
    <shape>
      <size android:height="2.0dp" />
      <solid android:color="#6c6a6c" />
    </shape>
  </item>
  <item android:id="@android:id/secondaryProgress">
    <clip>
      <shape>
        <size android:height="2.0dp" />
        <solid android:color="#fffae3" />
      </shape>
    </clip>
  </item>
  <item android:id="@android:id/progress">
    <clip>
      <shape>
        <size android:height="2.0dp" />
        <gradient android:startColor="@android:color/system_accent1_300"
                  android:endColor="@android:color/system_accent1_100"
                  android:angle="0.0"
                  android:type="linear" />
      </shape>
    </clip>
  </item>
</layer-list>